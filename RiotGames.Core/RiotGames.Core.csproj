<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net5.0;net6.0</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>RiotGames</RootNamespace>
    <PackageId>MikaelDui.RiotGames.Core</PackageId>
    <Authors>Mikael DÃºi Bolinder</Authors>
    <Product>MikaelDui.RiotGames.Core</Product>
    <Description>Core models for the MikaelDui.RiotGames.* packages</Description>
    <PackageProjectUrl>https://github.com/mikaeldui/riot-games-dotnet-core</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageIcon>Icon.png</PackageIcon>
    <RepositoryUrl>https://github.com/mikaeldui/riot-games-dotnet-core</RepositoryUrl>
    <PackageTags>RiotGames</PackageTags>
    <PackageLicenseExpression>Unlicense</PackageLicenseExpression>
    <AssemblyName>MikaelDui.RiotGames.Core</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\Icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MikaelDui.Text.Json.Serialization" Version="1.0.1" />
  </ItemGroup>

</Project>
